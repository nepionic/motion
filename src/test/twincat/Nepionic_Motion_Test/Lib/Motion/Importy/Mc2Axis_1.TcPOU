END_VAR
VAR
	command_pointer : POINTER TO HaltCommand;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[command_pointer := __NEW(
	HaltCommand(
		axis := THIS^,
		deceleration := deceleration,
		jerk := jerk,
		buffer_mode := buffer_mode
	)
);

Halt := command_pointer^;]]></ST>
      </Implementation>
    </Method>
