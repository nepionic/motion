        <Implementation>
          <ST><![CDATA[CASE axis_ref.Status.MotionState OF
	Tc2_Mc2.Mc_AxisState_Disabled: MotionState := AxisMotionState.Disabled;
	Tc2_Mc2.Mc_AxisState_Standstill: MotionState := AxisMotionState.Standstill;
	Tc2_Mc2.Mc_AxisState_ErrorStop: MotionState := AxisMotionState.ErrorStop;
	Tc2_Mc2.Mc_AxisState_Stopping: MotionState := AxisMotionState.Stopping;
	Tc2_Mc2.Mc_AxisState_Homing: MotionState := AxisMotionState.Homing;
	Tc2_Mc2.Mc_AxisState_DiscreteMotion: MotionState := AxisMotionState.DiscreteMotion;
	Tc2_Mc2.Mc_AxisState_ContinousMotion: MotionState := AxisMotionState.ContinuousMotion;
	Tc2_Mc2.Mc_AxisState_SynchronizedMotion: MotionState := AxisMotionState.SynchronisedMotion;
ELSE
	MotionState := AxisMotionState.Undefined;
END_CASE]]></ST>
        </Implementation>
      </Get>
