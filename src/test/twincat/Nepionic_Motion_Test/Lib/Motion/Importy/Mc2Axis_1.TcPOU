    <Method Name="GearIn" Id="{79db61e5-b6e4-4a42-a61c-fde9fffbf29e}" FolderPath="Motion\Coupling\">
      <Declaration><![CDATA[METHOD PUBLIC GearIn : AsyncCommand
VAR_INPUT
	master_axis : Axis;
	numerator : LREAL;
	denominator : UINT := 1;
	//acceleration : LREAL := 0;
	//deceleration : LREAL := 0;
	//jerk : LREAL := 0;
	//buffer_mode	: MotionBufferMode := MotionBufferMode.Aborting;
END_VAR
VAR
	mc2_master_axis : POINTER TO Mc2Axis;
	command_pointer : POINTER TO GearInCommand;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF __QUERYPOINTER(master_axis, mc2_master_axis) THEN
	command_pointer := __NEW(
		GearInCommand(
			master_axis := mc2_master_axis^,
			slave_axis := THIS^,
			numerator := numerator,
			denominator := denominator
		)
	);
	
	GearIn := command_pointer^;
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="GearOut" Id="{185a3852-a51e-46f6-bf05-86308c0b02ed}" FolderPath="Motion\Coupling\">
