    </Method>
    <Method Name="BuildStopCommand" Id="{b610c3c1-e30d-45a1-a3a3-8fb000d29513}" FolderPath="Builders\">
      <Declaration><![CDATA[METHOD PUBLIC BuildStopCommand : MotionCommand
VAR_INPUT
	axis : REFERENCE TO Mc2Axis;
END_VAR
VAR
	command_ptr : POINTER TO StopCommand;
	command : MotionCommand;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[command_ptr := __NEW(StopCommand(
	axis := axis,
	deceleration := parameters.deceleration,
	jerk := parameters.jerk
));
BuildStopCommand := command_ptr^;]]></ST>
      </Implementation>
    </Method>
