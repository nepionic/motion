  <POU Name="DefaultMoveBuilder" Id="{7cc6e411-6752-4f2e-99f0-f4438d1a908a}" SpecialFunc="None">
    <Declaration><![CDATA[{attribute 'linkalways'} 
{attribute 'no_explicit_call' := 'This FB is a CLASS and must be accessed using methods or properties'}
{attribute 'enable_dynamic_creation'}
FUNCTION_BLOCK INTERNAL DefaultMoveBuilder IMPLEMENTS MoveBuilder
VAR
	parent_builder : MotionBuilder;
	parameters : REFERENCE TO BuilderParameters;
	move_velocity_builder : DefaultMoveVelocityBuilder(parent_builder := parent_builder, parameters := parameters);
	move_absolute_builder : DefaultMoveAbsoluteBuilder(parent_builder := parent_builder, parameters := parameters);
	move_relative_builder : DefaultMoveRelativeBuilder(parent_builder := parent_builder, parameters := parameters);
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Folder Name="Builders" Id="{34f540d6-92b4-474e-be32-fb01f8674988}" />
    <Folder Name="Constructor" Id="{52e2fcc3-a98f-4ee3-9141-a97ed05d40ba}" />
    <Folder Name="Property Setters" Id="{8d89c4dc-4006-4a63-a63f-c3833a9c4fb3}" />
    <Method Name="Acceleration" Id="{3ba4f4e3-87f3-4117-a0af-c1f0fda7c66b}" FolderPath="Property Setters\">
      <Declaration><![CDATA[METHOD PUBLIC Acceleration : MoveBuilder
VAR_INPUT
	value : LREAL;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[parameters.acceleration := value;
Acceleration := THIS^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="BufferMode" Id="{093cfb20-4e19-4b4c-a237-f1ac9b9fcb01}" FolderPath="Property Setters\">
