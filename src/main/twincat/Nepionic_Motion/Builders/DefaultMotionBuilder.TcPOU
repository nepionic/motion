	command : MotionCommand;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[command_ptr := __NEW(HaltCommand(
	axis := axis,
	deceleration := parameters.deceleration,
	jerk := parameters.jerk,
	buffer_mode := parameters.buffer_mode
));
BuildHaltCommand := command_ptr^;]]></ST>
