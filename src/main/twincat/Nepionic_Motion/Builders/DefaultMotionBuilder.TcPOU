VAR
	parameters : BuilderParameters;
	move_builder : DefaultMoveBuilder(parent_builder := THIS^, parameters := parameters);
	move_velocity_builder : DefaultMoveVelocityBuilder(parent_builder := THIS^, parameters := parameters);
	move_absolute_builder : DefaultMoveAbsoluteBuilder(parent_builder := THIS^, parameters := parameters);
	move_relative_builder : DefaultMoveRelativeBuilder(parent_builder := THIS^, parameters := parameters);
	gear_in_builder : DefaultGearInBuilder(parent_builder := THIS^, parameters := parameters);
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
